{
    "name": "todolist",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:json-server\" \"npm run dev:react\"",
        "dev:react": "vite",
        "dev:json-server": "json-server --watch db.json --port 5000",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@ant-design/icons": "^5.3.7",
        "antd": "^5.17.3",
        "axios": "^1.7.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "json-server": "^1.0.0-beta.1",
        "vite": "^5.2.0",
        "wait-on": "^7.2.0"
    }
}
